#
# CHANNEL 7: direct ADC
#
record(ai,"CH7:value") {
  field(DTYP,"ADC channel")
  field(SCAN,"1 second")
  field(INP,"@7")
  field(FLNK,"CH7:VOLT")
  field(MDEL,-1)
}

record(calc,"CH7:VOLT") {
  field(INPA,"CH7:value")
  field(CALC,"A / 2^21")
  field(EGU,"V")
  field(HOPR,2.5)
  field(LOPR,0)
  field(FLNK,"S1:RH")
  field(MDEL,-1)
}

record(calc,"S1:RH") {
  field(INPA,"CH7:VOLT")
  field(CALC,"A / 2.5 * 100")
  field(EGU,"%RH")
  field(HOPR,100)
  field(LOPR,0)
  field(MDEL,-1)
} 

