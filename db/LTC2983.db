#
# CHANNEL 2: thermistor 10K@25C
#
record(ai,"CH2:value") {
  field(DTYP,"ADC channel")
  field(SCAN,"1 second")
  field(INP,"@2")
  field(FLNK,"S1:TEMP01")
  field(MDEL,-1)
}

record(calc,"S1:TEMP01") {
  field(INPA,"CH2:value")
  field(CALC,"A / 1024")
  field(EGU,"degC")
  field(HOPR,150)
  field(LOPR,-55)
  field(HIGH,45)
  field(LOW,15)
  field(HIHI,115)
  field(LOLO,-35)
  field(FLNK,"S1:GAMMA01")
  field(MDEL,-1)
}

