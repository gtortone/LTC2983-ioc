#
# CHANNEL 7: direct ADC
#
record(ai,"CH7:value") {
  field(DTYP,"ADC channel")
  field(SCAN,"1 second")
  field(INP,"@7")
  field(FLNK,"CH7:VOLT")
  field(MDEL,-1)
}

record(calc,"CH7:VOLT") {
  field(INPA,"CH7:value")
  field(CALC,"A / 2^21")
  field(EGU,"V")
  field(HOPR,2.5)
  field(LOPR,0)
  field(FLNK,"S1:RH")
  field(MDEL,-1)
}

record(calc,"S1:RH") {
  field(INPA,"CH7:VOLT")
  field(CALC,"A / 2.5 * 100")
  field(EGU,"%")
  field(HOPR,100)
  field(LOPR,0)
  field(FLNK,"S1:RH:FANOUT")
  field(MDEL,-1)
}


#
# Dew point calculation depends on RH variation,
# this record fans the RH out to GAMMA records for S1
#
record(fanout,"S1:RH:FANOUT") {
  field(LNK1,"S1:GAMMA01")
  field(LNK2,"S1:GAMMA02")
  field(LNK3,"S1:GAMMA03")
  field(SELM,"All")
}

#
# Dew point calculation is split in S1:GAMMA and S1:DEW
# S1:TEMP01 
#
record(calc,"S1:GAMMA01") {
  field(INPA,"S1:TEMP01")
  field(INPB,"S1:RH")
  field(CALC,"LOGE(B/100) +(18.678 - A/234.5)*(A / (257.14+A)) ")
  field(FLNK,"S1:DEW01")
  field(MDEL,-1)
}

record(calc,"S1:DEW01") {
  field(INPA,"S1:GAMMA01")
  field(CALC,"257.14 * A /( 18.678 - A) ")
  field(EGU,"degC")
  field(HOPR,150)
  field(LOPR,-55)
  field(HIGH,45)
  field(LOW,15)
  field(HIHI,115)
  field(LOLO,-35)
  field(MDEL,-1)
}

