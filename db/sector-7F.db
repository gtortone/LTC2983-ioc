#
# CHANNEL 2: thermistor 10K@25C
#
record(ai,"CH2:value") {
  field(DTYP,"ADC channel")
  field(SCAN,"5 second")
  field(INP,"@2")
  field(FLNK,"S7F:TEMP01")
  field(MDEL,-1)
}

record(calc,"S7F:TEMP01") {
  field(INPA,"CH2:value")
  field(CALC,"A / 1024")
  field(EGU,"degC")
  field(HOPR,150)
  field(LOPR,-55)
  field(HIGH,45)
  field(LOW,15)
  field(HIHI,115)
  field(LOLO,-35)
  field(FLNK,"S7F:GAMMA01")
  field(MDEL,-1)
}

