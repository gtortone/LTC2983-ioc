#
# CHANNEL 13: diode
#
record(ai,"$(SNAME):CH13:value") {
  field(DTYP,"ADC channel")
  field(SCAN,"5 second")
  field(INP,"@13")
  field(FLNK,"$(SNAME):DIODE")
  field(MDEL,-1)
}

record(calc,"$(SNAME):DIODE") {
  field(INPA,"$(SNAME):CH13:value")
  field(CALC,"A / 1024")
  field(EGU,"degC")
  field(HOPR,150)
  field(LOPR,-55)
  field(HIGH,45)
  field(LOW,15)
  field(HIHI,115)
  field(LOLO,-35)
  field(MDEL,-1)
}

#
# CHANNEL 14: direct ADC
#
record(ai,"$(SNAME):CH14:value") {
  field(DTYP,"ADC channel")
  field(SCAN,"5 second")
  field(INP,"@14")
  field(FLNK,"$(SNAME):CH14:VOLT")
  field(MDEL,-1)
}

record(calc,"$(SNAME):CH14:VOLT") {
  field(INPA,"$(SNAME):CH14:value")
  field(CALC,"A / 2^21")
  field(EGU,"V")
  field(HOPR,1)
  field(LOPR,0)
  field(FLNK,"$(SNAME):RH")
  field(MDEL,-1)
}

record(calc,"$(SNAME):RH") {
  field(INPA,"$(SNAME):CH14:VOLT")
  field(CALC,"(A * 100)")
  field(EGU,"%")
  field(HOPR,100)
  field(LOPR,0)
  field(MDEL,-1)
}

